{
    "name": "vscode-switcher",
    "displayName": "Switcher",
    "description": "Switch to different file extension for vscode (e.g. foo.c -> bar.h)",
    "version": "0.0.1",
    "publisher": "blackmery",
    "license": "MIT",
    "bugs": { "url": "https://github.com/blackmery/vscode-switcher/issues" },
    "homepage": "https://github.com/blackmery/vscode-switcher",
    "repository": {
        "type": "git",
        "url": "https://github.com/blackmery/vscode-switcher.git"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.switcher.run"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.switcher.run",
            "title": "Switcher: Switch to Different File Extension"
        }],
        "keybindings": [{
            "command": "extension.switcher.run",
            "key": "ctrl+shift+c",
            "mac": "cmd+shift+c"
        }],
        "configuration": {
            "type": "object",
            "title": "Switcher configuration",
            "properties": {
                "switcher.findAllFilesInRootDirectory": {
                    "type": "boolean",
                    "default": false,
                    "description": "Swicher: findAllFilesInRootDirectory"
                },
                "switcher.findExtensionOrder": {
                    "type": "array",
                    "default": ["cpp", "c", "h", "hpp", "inl"],
                    "description": "Swicher: findExtensionOrder"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}